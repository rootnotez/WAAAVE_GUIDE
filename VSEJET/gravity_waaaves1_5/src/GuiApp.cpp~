	saveBuffer["MACRO0"]["BLOCK_1"]["selectMacro0Fb1MixAndKey"] = selectMacro0Fb1MixAndKey;
	saveBuffer["MACRO0"]["BLOCK_1"]["selectMacro0Fb1Geo1"] = selectMacro0Fb1Geo1;
