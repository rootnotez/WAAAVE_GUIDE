void GuiApp::selectMacro13Menu(){
	if(ImGui::BeginMenu("b1 ##macro13")){
		if(ImGui::BeginMenu("inputs ##macro13")){
			if(ImGui::BeginMenu("ch1 adjust ##macro13")){
				selectMacro13Fb1DelayTime=0; selectMacro13Fb2DelayTime=0;
