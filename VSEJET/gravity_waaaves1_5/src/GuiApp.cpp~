void GuiApp::selectMacro9Menu(){
	if(ImGui::BeginMenu("b1 ##macro9")){
		if(ImGui::BeginMenu("inputs ##macro9")){
			if(ImGui::BeginMenu("ch1 adjust ##macro9")){
				selectMacro9Fb1DelayTime=0; selectMacro9Fb2DelayTime=0;
