		if(ImGui::BeginMenu("fb1 ##macro1")){
			ImGui::RadioButton("none ##macro1",&selectMacro1Fb1DelayTime,0);
			ImGui::RadioButton("fb1 delay time ##macro1",&selectMacro1Fb1DelayTime,1);
