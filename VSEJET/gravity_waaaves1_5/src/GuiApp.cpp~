		ch1AdjustLfo[i]=0.0f;
		ch1AdjustLfoMidiActive[i]=0;
		
		ch2AdjustLfo[i]=0.0f;
		ch2AdjustLfoMidiActive[i]=0;
		
		//fb1 param
