					ImGui::RadioButton(block2ColorizeLfo2Names[k-1].c_str(),&selectMacro4Block2ColorizeLfo2,k);
				}
				ImGui::EndMenu();
			}
			if(ImGui::BeginMenu("block2 colorize lfo 3 ##macro4")){
				selectMacro4Fb1DelayTime=0; selectMacro4Fb2DelayTime=0;
