		block1ColorizeLfo3[i]=0.0f;
		block1ColorizeLfo3MidiActive[i]=0;
		
		//block2 params
		block2Geo[i]=0.0f;
		block2GeoMidiActive[i]=0;
		
		block2Colorize[i]=0.0f;
		block2ColorizeMidiActive[i]=0;
		
		block2Filters[i]=0.0f;
		block2FiltersMidiActive[i]=0;
		
		//block2 lfo
		block2Geo1Lfo1[i]=0.0f;
