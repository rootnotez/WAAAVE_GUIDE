	saveBuffer["MACRO0"]["BLOCK_1"]["selectMacro0Fb1DelayTime"] = selectMacro0Fb1DelayTime;
	
	saveBuffer["MACRO0"]["BLOCK_2"]["selectMacro0Block2InputAdjust"] = selectMacro0Block2InputAdjust;
