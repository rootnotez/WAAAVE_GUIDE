									for (int i = 0; i < PARAMETER_ARRAY_LENGTH; i++) {
										block2Filters[i] = 0.0f;
										block2FiltersMidiActive[i] = 0;
									}
									//ADD ALL CHECKBOXES ETC HERE
									block2FiltersReset = 0;
								}
								
								midi2Gui(block2FiltersMidiActive, block2Filters, block2FiltersMidiGui);
								
