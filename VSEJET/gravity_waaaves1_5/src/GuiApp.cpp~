void GuiApp::selectMacro6Menu(){
	if(ImGui::BeginMenu("b1 ##macro6")){
		if(ImGui::BeginMenu("inputs ##macro6")){
			if(ImGui::BeginMenu("ch1 adjust ##macro6")){
				selectMacro6Fb1DelayTime=0; selectMacro6Fb2DelayTime=0;
