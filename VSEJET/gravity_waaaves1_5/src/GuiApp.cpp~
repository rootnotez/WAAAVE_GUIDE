					}
					ImGui::RadioButton(matrixMixLfo1Names[k-1].c_str(),&selectMacro4MatrixMixLfo1,k);
				}
				ImGui::EndMenu();
			}
			if(ImGui::BeginMenu("matrix mix lfo 2 ##macro4")){
				selectMacro4Fb1DelayTime=0; selectMacro4Fb2DelayTime=0;
				selectMacro4Ch2MixAndKey=0;
